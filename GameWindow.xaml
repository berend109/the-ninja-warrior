<Window x:Class="project_arcade.GameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:project_arcade"
        mc:Ignorable="d"
        Title="GameWindow" Height="450" Width="800" WindowState="Maximized" WindowStyle="None">
    <Canvas>
        <Button Height="20" Width="50" Click="ReturnToMenuBtn">Return</Button>
        <Rectangle Height="75" Width="400" Fill="LightGray" Canvas.Bottom="75" Canvas.Right="0"></Rectangle>
        <Rectangle Height="75" Width="400" Fill="LightGray" Canvas.Bottom="0"></Rectangle>
        <Rectangle x:Name="Player" Height="150" Width="75" Fill="CornflowerBlue" Canvas.Left="100" Canvas.Bottom="75"></Rectangle>
    </Canvas>
</Window>
