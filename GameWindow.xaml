<Window x:Class="project_arcade.GameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:project_arcade"
        mc:Ignorable="d"
        Title="GameWindow" WindowState="Maximized" WindowStyle="None">
    <Canvas Name="gameCanvas" Focusable="True" KeyDown="KeyIsDown" KeyUp="KeyIsUp">
        <Button Height="20" Width="50" Focusable="false" Click="ReturnToMenuBtn">Return</Button>
        <Rectangle Height="75" Width="400" Tag="Platform" Fill="LightGray" Canvas.Bottom="75" Canvas.Right="0"></Rectangle>
        <Rectangle Height="75" Width="400" Tag="Platform" Fill="LightGray" Canvas.Bottom="0"></Rectangle>
        <Rectangle x:Name="Player" Height="150" Width="75" Fill="CornflowerBlue" Canvas.Left="55" Canvas.Bottom="75" Canvas.Top="152"></Rectangle>
    </Canvas>
</Window>
